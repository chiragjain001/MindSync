# Copy this file to .env.local and fill in your Supabase credentials.
# Note: .env* files are gitignored in this repo. Keep secrets out of version control.

NEXT_PUBLIC_SUPABASE_URL= 
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Optional: Google OAuth (configure these in Supabase Dashboard → Authentication → Providers → Google)
# These are not used by the frontend code directly but are useful placeholders for your records.
SUPABASE_GOOGLE_CLIENT_ID=
SUPABASE_GOOGLE_CLIENT_SECRET=
